{# begin {templatelocation} #}
<div class="uk-align-right uk-margin">
	<form uk-form action="{action}" method="get" name="jumpbox" onsubmit="if(document.jumpbox.forum.value == -1){return false;}">
		<select class="uk-select uk-form-small uk-form-width-small" name="forum" title="{LANG_msg106}" onchange="this.form.submit();">
			<option selected="selected" value="-1">{LANG_msg106}</option>
			{selecthtml}
		</select>
	</form>
</div>
{# end {templatelocation} #}
