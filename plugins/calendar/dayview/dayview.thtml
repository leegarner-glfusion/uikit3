{# begin {templatelocation} #}
<div class="uk-panel uk-panel-box uk-margin-remove">
	<div uk-grid class="uk-child-width-1-3 uk-visible@m">
		<div class="uk-text-center">
			<span class="uk-text-bold">{lang_day}</span>
		</div>
		<div class="uk-text-center">
			<a href="{site_url}/calendar/index.php?mode={mode}&amp;view=week" rel="nofollow">{lang_week}</a>
		</div>
		<div class="uk-text-center">
			<a href="{site_url}/calendar/index.php?mode={mode}" rel="nofollow">{lang_month}</a>
		</div>
	</div>
	<div class="uk-margin-remove uk-hidden@m">
		<form id="viewselectform" uk-form action="{site_url}/calendar/index.php" method="get">
			<label class="uk-form-label">{$LANG_CAL_1[53]}</label>
			<input type="hidden" name="mode" value="{mode}">
			<select class="uk-select uk-form-small uk-form-width-xsmall" id="viewselect" name="view"  onchange="this.form.submit()">
				<option value="day" selected="selected">{lang_day}</option>
				<option value="week">{lang_week}</option>
				<option value="month">{lang_month}</option>
			</select>
		</form>
	</div>
</div>
<div class="uk-panel">
	<div class="uk-text-muted">
		<a href="{site_url}/calendar/index.php?mode={mode}&amp;view=day&amp;month={prevmonth}&amp;day={prevday}&amp;year={prevyear}">&lt;&lt;</a>&nbsp;&nbsp;{currentday}&nbsp;&nbsp;<a href="{site_url}/calendar/index.php?mode={mode}&amp;view=day&amp;month={nextmonth}&amp;day={nextday}&amp;year={nextyear}" rel="nofollow">&gt;&gt;</a>
	</div>
	<div>
		{calendar_title} {calendar_toggle}
	</div>
</div>
<table class="uk-table uk-table-striped uk-table-hover">
	<tr>
		<td>
			{lang_week} {week_num}
		</td>
		<td>
			{allday_events}
		</td>
	</tr>
	<tr>
		<td>{0_hour}</td>
		<td>{0_cols}</td>
	</tr>
	<tr>
		<td>{1_hour}</td>
		<td>{1_cols}</td>
	</tr>
	<tr>
