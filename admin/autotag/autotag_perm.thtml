<form id="autotag-permission" class="uk-form uk-margin" action="{s_form_action}" method="post">
	<input type="hidden" id="autotag_id" name="autotag_id" value="{var_autotag_id}">
	<input type="hidden" name="{input_csrf_token}" value="{var_token}">
	{admin_list}
	<div class="uk-margin">
		<button class="uk-button uk-button-primary" type="submit" value="{$LANG_ADMIN[save]}" name="psave">{$LANG_ADMIN[save]}</button>
		<button class="uk-button uk-button-danger" type="submit" value="{$LANG_ADMIN[cancel]}" name="cancel">{$LANG_ADMIN[cancel]}</button>
	</div>
</form>